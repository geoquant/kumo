# Anthropic API Key (required)
ANTHROPIC_API_KEY=your_api_key_here

# Component Registry URL (optional - defaults to Kumo)
# Change this to use any component library with a JSON registry
REGISTRY_URL=https://raw.githubusercontent.com/cloudflare/kumo/main/packages/kumo/ai/component-registry.json

# Registry Cache Duration in milliseconds (optional - default: 3600000 = 1 hour)
REGISTRY_CACHE_DURATION=3600000

# Examples for other component libraries:
# REGISTRY_URL=https://example.com/components/registry.json
# REGISTRY_URL=https://raw.githubusercontent.com/your-org/your-ui-lib/main/registry.json
# REGISTRY_URL=https://cdn.example.com/component-registry.json
