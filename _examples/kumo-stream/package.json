    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "4.1.17",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^4.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "tailwindcss": "4.1.17",
    "tsx": "^4.19.0",
    "typescript": "^5.9.2",
    "vite": "7.2.4",
